<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Game_Date_Must_Be_Future</fullName>
    <active>false</active>
    <errorConditionFormula>Game_Date__c &lt; TODAY()</errorConditionFormula>
    <errorDisplayField>Game_Date__c</errorDisplayField>
    <errorMessage>Data gry musi być w przyszłości</errorMessage>
</ValidationRule>
